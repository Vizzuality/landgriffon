{
  "name": "@landgriffon/api",
  "version": "0.2.0",
  "description": "API service for the LandGriffon platform",
  "author": "Vizzuality <hello@vizzuality.com> (https://vizzuality.com/)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_OPTIONS=\"--max-old-space-size=2048\" NODE_PATH=dist/src node dist/src/main",
    "build": "rm -rf dist && tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config test/jest-config.json -i",
    "test:watch": "jest --config test/jest-config.json --watch",
    "test:cov": "jest --config test/jest-config.json --coverage",
    "test:ci": "jest --config test/jest-config.json --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest-config.json --runInBand"
  },
  "engines": {
    "node": "^16.13"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "~3.3.2",
    "@nestjs/axios": "~0.0.3",
    "@nestjs/bull": "~0.4.2",
    "@nestjs/common": "~8.2.1",
    "@nestjs/core": "~8.2.1",
    "@nestjs/jwt": "~8.0.0",
    "@nestjs/passport": "~8.0.1",
    "@nestjs/platform-express": "~8.2.1",
    "@nestjs/swagger": "~5.1.4",
    "@nestjs/terminus": "~8.0.1",
    "@nestjs/typeorm": "~8.0.2",
    "bcrypt": "~5.0.1",
    "bull": "~4.1.1",
    "class-transformer": "<=0.4.0",
    "class-validator": "~0.13.1",
    "compression": "~1.7.4",
    "config": "~3.3.6",
    "helmet": "~4.6.0",
    "jsonapi-serializer": "~3.6.7",
    "lodash": "~4.17.21",
    "ms": "~2.1.3",
    "multer": "~1.4.3",
    "nestjs-base-service": "~0.8.2",
    "passport": "~0.5.0",
    "passport-jwt": "~4.0.0",
    "passport-local": "~1.0.0",
    "pg": "~8.7.1",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~7.4.0",
    "swagger-ui-express": "~4.1.6",
    "typeorm": "0.2.41",
    "uuid": "~8.3.2",
    "xlsx": "~0.17.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.5",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.40",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/jsonapi-serializer": "^3.6.5",
    "@types/lodash": "^4.14.177",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.7",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "jest": "^27.3.1",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.5.2"
  }
}
