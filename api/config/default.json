{
  "server": {
    "port": 3000,
    "loggerLevel": [
      "error",
      "warn",
      "log"
    ]
  },
  "auth": {
    "requireUserAuth": true,
    "requireUserAccountActivation": true,
    "signUpIsPublic": true,
    "jwt": {
      "expiresIn": "2h",
      "secret": null
    },
    "password": {
      "minLength": 6,
      "includeNumerics": false,
      "includeUpperCase": false,
      "includeSpecialCharacters": false
    }
  },
  "import": {
    "missingDataFallbackStrategy": "fallback"
  },
  "redis": {
    "host": "localhost",
    "port": 6379
  },
  "geocodingCache": {
    "enabled": true,
    "TTL": 86400
  },
  "db": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "landgriffon",
    "password": "",
    "database": "landgriffon",
    "synchronize": false,
    "migrationsRun": true,
    "dropSchema": false,
    "logging": true
  },
  "fileUploads": {
    "sizeLimit": 5e6,
    "storagePath": "/tmp/csv-uploads"
  },
  "geolocation": {
    "gmapsApiKey": null
  }
}
